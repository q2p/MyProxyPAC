{
	"name": "EnfCon",
	"description": "Manages proxy settings and security of requests.",
	"version": "1.0",
	"manifest_version":	2,
	"permissions": [ "storage", "*://*/", "webRequest", "webRequestBlocking" ],
	"icons": { "128":	"icon.png" },
	"options_page": "options.html",
	"minimum_chrome_version": "17",
	"browser_action": {
		"default_popup": "popup.html"
	},
	"background":	{
		"scripts": [ "background.js" ],
		"persistent": true
	}
}
